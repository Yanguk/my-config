{
  "version": "0.2",
  "dictionaries": ["neovim", "en"],
  "ignorePaths": ["node_modules", ".git", ".vscode"],
  "ignoreRegExpList": ["\\b\\d+[a-z]{1,2}\\b"],
  "allowCompoundWords": true,
  "words": [
    "stylua",
    "rustfmt",
    "ngrok",
    "chadrc",
    "nvimtree",
    "deno",
    "clangd",
    "nvterm",
    "bufnr",
    "bufs"
  ],
  "language": "en",
  "dictionaryDefinitions": [
    { "name": "neovim", "addWords": true, "path": ".\/my-dictionary.txt" }
  ]
}
